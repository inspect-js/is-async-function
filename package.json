  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^1.2.0",
    "is-match": "^0.4.1",
    "mukla": "^0.4.1",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.4.0",
    "standard-version": "^2.4.0"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
